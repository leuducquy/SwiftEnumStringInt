                             "COMPENSATORY",
                             "SPECIAL",
                             "OTHER"]
let offerTypeArray = ["ABSENT", "OFFERWORK", "OVERTIMER"]
let subTypeOffWork = ["WORK","NOWORK","OTHER"]
enum CSOfferType : Int{
    case ABSENT = 1
    case OFFERWORK
    case OVERTIMER
    func description()->String{
        switch self {
        case .ABSENT:
            return "ABSENT"
            
        case .OFFERWORK:
            return"OFFERWORK"
            
        case .OVERTIMER:
            return"OVERTIMER"
        
    
        }
    }
   }
enum CSSubTypeAbsent : Int {
    case ALL = 1
    case MORNING = 2
    case AFTERNOON = 3
    case COMPENSATORY = 4
    case SPECIAL = 5
    case OTHER = 9
    
    func description()->String{
        switch self {
        case .ALL:
            return "ALL"
            
        case .MORNING:
            return "MORNING"
            
        case .AFTERNOON:
            return"AFTERNOON"
        case .COMPENSATORY :
            return"COMPENSATORY"
            
        case .SPECIAL :
            return"SPECIAL"
            
        case .OTHER :
            return"OTHER"
        }
    }

    
}
enum CSSubTypeOther  : Int{
    case WORK = 1
    case NOWORK
    case OTHER
    func description()->String{
        switch self {
        case .WORK:
            
            return "WORK"
            
        case .NOWORK:
            
            return "NOWORK"
            
        case .OTHER:
            return"OTHER"
       
        }
    }

}




